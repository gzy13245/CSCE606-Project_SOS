<h1>Welcome to login!</h1>
<% form_tag do %>
    <table>
      <tr>
        <td>User name:</td>
        <td><%= text_field("user", "username") %></td>
      </tr>
      <tr>
        <td>Password:</td>
        <td><%= password_field("user", "hashed_password") %></td>
      </tr>
      <tr>
        <td></td>
        <td><input type="submit" value=" login " /> <input type="reset" value=" reset " /></td>
      </tr>
    </table>
<% end %>