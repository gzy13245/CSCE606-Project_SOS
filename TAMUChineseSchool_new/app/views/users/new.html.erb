<%= form_for :user, url: users_path  do |f| %>
    <p>
      <%= f.label :last_name %><br>
      <%= f.text_field :last_name %>
    </p>

    <p>
      <%= f.label :last_name_chn %><br>
      <%= f.text_field :last_name_chn %>
    </p>

    <p>
      <%= f.label :first_name %><br>
      <%= f.text_field :first_name %>
    </p>

    <p>
      <%= f.label :first_name_chn %><br>
      <%= f.text_field :first_name_chn %>
    </p>

    <p>
        <%= f.select :gender, ['Male', 'Female'] %>
    </p>

    <p>
      <%= f.label :address %><br>
      <%= f.text_area :address %>
    </p>

    <p>
      <%= f.label :phone %><br>
      <%= f.text_field :phone %>
    </p>

    <p>
      <%= f.label :email %><br>
      <%= f.text_field :email %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>