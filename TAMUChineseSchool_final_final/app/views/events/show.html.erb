<p id="notice"><%= notice %></p>
<div><%= link_to 'Back To Event List', events_path %></div>
<h1><%= @event.event_name %></h1>
<br/>
<div class="row">

  <div class="col-md-6">
    <table class="table table-striped">
      <tr><td>Year:</td><td><%= @event.year %></td></tr>
      <tr><td>Semester:</td><td><%= @event.semester %></td></tr>
      <tr><td>Event Name:</td><td><%= @event.event_name %></td></tr>
      <tr><td>Place:</td><td><%= @event.place %></td></tr>
      <tr><td>Time:</td><td><%= @event.time %></td></tr>
      <tr><td>Description:</td><td><%= @event.event_description %></td></tr>
    </table>
  </div>
</div>
<br />
