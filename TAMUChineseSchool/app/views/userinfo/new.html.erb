<%= form_for :userinfo do |f| %>
    <table>
    <tr>
      <td> <%= f.label :Email %></td>
      <td><%= f.text_field :title %></td>
    </tr>

    <tr>
      <td><%= f.label :Password %></td>
      <td><%= f.text_area :text %></td>
    </tr>

    <tr>
      <td><%= f.label :"Confirm Password" %></td>
      <td><%= f.text_area :text %></td>
    </tr>
    </table>

    <p>
      <%= f.submit value=" Register " %>
    </p>

<% end %>
